{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "zip": {
      "outputs": [".output/**"]
    },
    "test:cov": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    }
  },
  "extends": ["//"]
}
